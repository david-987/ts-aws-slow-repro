{
    "plugins": ["./node_modules/prettier-plugin-jsdoc"],
    "$schema": "http://json.schemastore.org/prettierrc",
    "jsdocSingleLineComment": false,
    "printWidth": 100,
    "endOfLine": "lf",
    "arrowParens": "always",
    "quoteProps": "as-needed",
    "bracketSpacing": true,
    "singleQuote": true,
    "tabWidth": 4,
    "semi": true,
    "trailingComma": "es5"
}
