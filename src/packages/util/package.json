{
    "name": "@packages/util",
    "version": "1.0.0",
    "lockfileVersion": 2,
    "scripts": {
        "compile": "tsc -b tsconfig.esm.json tsconfig.json"
    },
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/esm/index.js"
        },
        "./core": {
            "require": "./dist/core/index.js",
            "import": "./dist/esm/core/index.js"
        }
    },
    "types": "dist/index.d.ts",
    "typesVersions": {
        "*": {
            "*": [
                "./dist/index.d.ts"
            ],
            "core": [
                "./dist/core/index.d.ts"
            ]
        }
    }
}
